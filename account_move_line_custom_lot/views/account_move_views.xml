<odoo>
	<data>
		<record id="view_move_form_lot" model="ir.ui.view">
			<field name="name">view.move.form.lot</field>
			<field name="model">account.move</field>
			<field name="inherit_id" ref="account.view_move_form" />
			<field name="arch" type="xml">
				<xpath expr="//page[@id='invoice_tab']//field[@name='account_id']" position="after">
					<field name="lot_id"/>
				</xpath>
			</field>
		</record>
	</data>
</odoo>